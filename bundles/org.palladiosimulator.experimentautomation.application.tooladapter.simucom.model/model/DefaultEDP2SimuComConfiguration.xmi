<?xml version="1.0" encoding="ASCII"?>
<simucomtooladapter:SimuComConfiguration
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ExperimentAutomation.Experiments.AbstractSimulation="http://palladiosimulator.org/ExperimentAutomation/AbstractSimulation/1.0"
    xmlns:simucomtooladapter="http://palladiosimulator.org/ExperimentAutomation/ToolAdapter/SimuCom/1.0"
    name="Default EDP2 SimuCom Configuration">
  <stopConditions
      xsi:type="ExperimentAutomation.Experiments.AbstractSimulation:MeasurementCountStopCondition"
      measurementCount="1000"/>
  <stopConditions
      xsi:type="ExperimentAutomation.Experiments.AbstractSimulation:SimTimeStopCondition"
      simulationTime="-1"/>
  <datasource
      xsi:type="ExperimentAutomation.Experiments.AbstractSimulation:FileDatasource"
      location="C:/data/experimentAutomation"/>
</simucomtooladapter:SimuComConfiguration>
